<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.shanto.teachersdashboard">

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="Teacher's Dashboard"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name="com.shanto.teachersdashboard.Reports_Detail_Activity"
            android:theme="@style/AppTheme2" />
        <activity android:name="com.shanto.teachersdashboard.Reports_Activity" />
        <activity
            android:name="com.shanto.teachersdashboard.ClassDetail_Activity"
            android:theme="@style/FullScreenTheme" />
        <activity
            android:name="com.shanto.teachersdashboard.Insert_class_Activity"
            android:theme="@style/AppTheme2" />
        <activity android:name="com.shanto.teachersdashboard.MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>